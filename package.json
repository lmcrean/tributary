{
  "name": "amplify-vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --exclude e2e/**/*",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:parallel": "playwright test --workers=4",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:prod": "cross-env PLAYWRIGHT_BASE_URL=https://main.d1l70uc5e2kcii.amplifyapp.com playwright test",
    "test:e2e:auth": "playwright test e2e/auth/auth.spec.ts",
    "test:e2e:button-modal": "playwright test e2e/delete-account/delete-account-button-and-modal.spec.ts",
    "test:e2e:delete-success": "playwright test e2e/delete-account/delete-account-success.spec.ts",
    "test:e2e:button-modal:prod": "cross-env PLAYWRIGHT_BASE_URL=https://main.d1l70uc5e2kcii.amplifyapp.com playwright test e2e/delete-account/delete-account-button-and-modal.spec.ts",
    "test:e2e:button": "playwright test e2e/delete-account/delete-account-button.spec.ts",
    "test:e2e:button:prod": "cross-env PLAYWRIGHT_BASE_URL=https://main.d1l70uc5e2kcii.amplifyapp.com playwright test e2e/delete-account/delete-account-button.spec.ts",
    "create-test-user": "node scripts/create-test-user.js",
    "check-test-user": "node scripts/check-test-user.js"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.9.1",
    "aws-amplify": "^6.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.3",
    "react-toastify": "^11.0.3"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.14.0",
    "@aws-amplify/backend-cli": "^1.4.8",
    "@aws-amplify/cli": "^12.14.1",
    "@aws-sdk/client-cognito-identity-provider": "^3.738.0",
    "@parcel/watcher": "^2.5.0",
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "aws-cdk": "^2.138.0",
    "aws-cdk-lib": "^2.138.0",
    "constructs": "^10.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^24.0.0",
    "playwright": "^1.42.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.4.10",
    "vitest": "^3.0.4"
  }
}
